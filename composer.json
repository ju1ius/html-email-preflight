{
  "name": "ju1ius/html-email-preflight",
  "type": "library",
  "description": "Preflight for Html Email",
  "keywords": ["html", "email", "css"],
  "homepage": "https://github.com/ju1ius/html-email-preflight",
  "license": "LGPLv2",
  "authors": [
    {
    "name": "ju1ius",
    "homepage": "https://github.com/ju1ius"
    }
  ],
  "repositories": [
    {
    "type": "vcs",
    "url": "http://github.com/zendframework/zf2"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "ju1ius/libphp": "dev-master",
    "ju1ius/css-parser": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "ju1ius": "lib/"
    }
  },
  "config": {
    "vendor-dir": "lib/vendor",
    "bin-dir": "lib/vendor/bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    }
  }
}

